import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int x;
		int y;
		
		x = s.nextInt();
		y = s.nextInt();
		
		
		// 1사분면: x > 0, y > 0
		// 2사분면: x < 0, y > 0
		// 3사분면: x < 0, y < 0
		// 4사분면: x > 0, y < 0
		if (y % 4 == 0 && (y % 100 != 0 || y % 400 == 0)) {
			System.out.println("1");
		}else {
			System.out.println("0");
		}
	}
}


