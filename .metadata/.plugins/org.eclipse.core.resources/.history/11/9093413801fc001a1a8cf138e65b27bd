
public class LinkedListRunner {

}

class LinkedList {
	Node header;
	
	public LinkedList() {
		Node header = new Node();
	}
	
	void append(int d) {
		Node end = new Node(d);
	}
	class Node {
		int data;
		Node next;
		public Node() {
			super();
		}
		public Node(int data) {
			super();
			this.data = data;
			this.next = null;
		}
	}
	
	
}
