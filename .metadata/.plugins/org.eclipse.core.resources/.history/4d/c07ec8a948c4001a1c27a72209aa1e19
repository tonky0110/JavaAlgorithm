import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int x;
		int y;
		
		int h = s.nextInt();
		int m = s.nextInt();
		
		h = m >= 45 ? h : (h-1);
		h = h > 0 ? h : (h+24);
		m = m >= 45 ? m-45 : (m+60-45);
		
		System.out.println(h);
		
		System.out.println(h + " " + m);
		
	}
}


