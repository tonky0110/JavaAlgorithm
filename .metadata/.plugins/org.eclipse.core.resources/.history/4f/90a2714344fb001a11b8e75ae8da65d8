package com.tonky.util;

class LinkedList {
	Node header;
	
	static class Node {
		int data;
		Node next;
		
		Node() {
			data = 0;
			next = null;
		}
		Node(int d) {
			data = d;
			next = null;
		}
	}
	
	LinkedList() {
		header = new Node();
	}
	
	void append(int d) {
		Node end = new Node(d);
		Node n = header;
		while (n.next != null) {
			n = n.next;
		}
		n.next = end;
	}
	
	void delete(int d) {
		Node n = header;
		while(n.next != null) {
			if (n.next.data == d) {
				n.next = n.next.next;
			} else {
				n = n.next;
			}
		}
	}
}
public class RemoveDups {

}
