
public class LinkedList {
	
	public static void main(String[] args) {
		Node head = new Node(1); 
	}
}
public class Node {
	int data;
	Node next;
	public Node() {
		super();
		this.data = 0;
		this.next = null;
	}
	public Node(int data) {
		super();
		this.data = data;
		this.next = null;
	}
	public Node(int data, Node next) {
		super();
		this.data = data;
		this.next = next;
	}
	public int getData() {
		return data;
	}
	public void setData(int data) {
		this.data = data;
	}
	public Node getNext() {
		return next;
	}
	public void setNext(Node next) {
		this.next = next;
	}
	void append(int d) {
		Node end = new Node(d);
		Node n = this;
		while(n.next != null) {
			n = n.next;
		}
		n.next = end;
	}
	void delete(int d) {
		Node n = this;
		while(n.next != null) {
			if(n.next.data == d) {
				n.next = n.next.next;
			} else {
				n = n.next;
			}
		}
	}
	void retrieve() {
		Node n = this;
		while(n.next != null) {
			System.out.print(n.next.data + " -> ");
			n = n.next;
		}
		System.out.println(n.data);
	}
}